{
  "name": "graphql-factory",
  "version": "3.0.0-alpha.0",
  "description": "Compose GraphQL types & schemas + middleware",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphql-factory/graphql-factory.git"
  },
  "options": {
    "mocha": "--check-leaks --full-trace src/**/__tests__/**/*-test.js",
    "lodash": "assign,capitalize,castArray,filter,find,forEach,get,has,identity,includes,intersection,isArray,isFunction,isNumber,isObject,isString,keys,map,noop,omit,reduce,set,union,uniq,values,without"
  },
  "scripts": {
    "lint": "eslint src",
    "build:lodash": "lodash modularize exports=es include=$npm_package_options_lodash -o src/common/lodash",
    "build:src": "rollup -c build/rollup.config.js && uglifyjs index.js > index.min.js",
    "build": "npm run lint && npm run build:src && npm run test",
    "test": "babel-node ./node_modules/.bin/_mocha $npm_package_options_mocha"
  },
  "author": "Branden Horiuchi <bhoriuchi@gmail.com>",
  "peerDependencies": {
    "graphql": ">=0.9.0"
  },
  "dependencies": {
    "graphql-factory-plugin": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "babelrc-rollup": "^3.0.0",
    "chai": "^4.1.2",
    "chalk": "^2.0.1",
    "eslint": "^4.8.0",
    "eslint-plugin-babel": "^4.1.2",
    "graphql": "^0.11.6",
    "lodash": "^4.17.4",
    "lodash-cli": "^4.17.4",
    "mocha": "^3.2.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "uglify-js": "^3.1.3"
  },
  "bugs": {
    "url": "https://github.com/graphql-factory/graphql-factory/issues"
  },
  "homepage": "https://github.com/graphql-factory/graphql-factory#readme",
  "directories": {
    "example": "example"
  },
  "keywords": [
    "graphql",
    "factory",
    "types",
    "json",
    "schema",
    "build",
    "extend",
    "merge",
    "gql",
    "facebook",
    "graph",
    "define",
    "definition",
    "es6",
    "helper"
  ]
}

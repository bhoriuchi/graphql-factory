    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "babelrc-rollup": "^3.0.0",
    "chai": "^4.1.2",
    "chalk": "^2.0.1",
    "eslint": "^4.8.0",

  "description": "Compose GraphQL objects with JSON definitions",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphql-factory/graphql-factory.git"
  },
  "options": {
    "mocha": "--check-leaks --full-trace src/**/__tests__/**/*-test.js"
